          <view class="right goods-title " data-dpsjhym="{{goods['maijia']['title']}}" bindtap="sjdp">
            进店看看
          </view>
          
            <text class="goods-price">【{{goods['maijia']['dpmc']}}】</text>
          </view>

        </view>
        
        <view class="flex-item3">
                    <button class="search-text" open-type="share" style="width:185rpx;color:#333;">分享</button>
                </view>
