<!-- <text bindtap="toggleDialog">请选择购买数量</text> -->
<view class="dialog {{ showDialog ? 'dialog--show' : '' }}">
  <view class="dialog__mask" bindtap="toggleDialog">点击这里继续浏览</view>
  <!---背景色--->
  <view class="dialog__container">
    <view class="row">
      <image bindtap="closeDialog" class="image-close" src="/image/delete.png"></image>
      <image class="image-sku" src="https://1681.qunqing168.com{{goods.titlepic}}"></image>
      <view class="column">
        <text class="sku-price" decode="{{true}}">￥{{goods.price}} &nbsp;&nbsp;&nbsp;&nbsp; 总价： {{totalprice}}</text>
        <text class="sku-title">库存 : {{goods.pmaxnum}} 件 销量 : {{goods.psalenum}} 件</text>
        <text class="sku-title">商品编码：{{goods.productno}}</text>
      </view>
    </view>
    <text class="border-line"></text>
    <view class="row">
      <text decode="{{true}}">&nbsp;&nbsp; 购买数量</text>
      <view class="quantity-position">
        <!-- <template is="quantity" data="{{ ...quantity1, componentId: 'quantity1' }}" /> -->
        <view class="goods-box">
        
          <view class="cart-count-box">
            <text class="cart-count-down" bindtap="minusCount">-</text>
            <text class="cart-count-num">{{num}}</text>
            <text class="cart-count-add" bindtap="addCount">+</text>
          </view>

        </view>
      </view>
    </view>
    <text class="border-line"></text>
    <view class="row">
      <text decode="{{true}}">&nbsp;&nbsp; 色彩</text>
      <view class="quantity-position">
        红色 黄色 绿色 蓝色 黑色
      </view>
    </view>
    <text class="border-line"></text>
    <view class="row">
      <text decode="{{true}}">&nbsp;&nbsp; 型号</text>
      <view class="quantity-position">
        xl xxl xxx
      </view>
    </view>
    <text class="border-line"></text>

    <button class="button-green" bindtap="addToCart" formType="submit">加入购物车</button>
    <button class="button-red" formType="submit">立即购买</button>
  </view>
</view>

