      <view class="goods-tab-nav {{curIndex === 0 ?'on':''}}" bindtap="bindTap" data-index="0">详 情</view>
      <view class="goods-tab-nav {{curIndex === 1 ?'on':''}}" bindtap="bindTap" data-index="1">参 数</view>
      <view class="goods-tab-nav {{curIndex === 2 ?'on':''}}" bindtap="bindTap" data-index="2">售 后</view>
      <view class="goods-tab-nav {{curIndex === 3 ?'on':''}}" bindtap="bindTaphaoping" data-index="3"
        data-maijia="{{goods['maijia']['title']}}" data-hpzt="" data-ksfyt="0">好 评</view>
    </view>
    <view wx:else class="goods-tab-box">
      <view class="goods-tab-nav {{curIndex === 0 ?'on':''}}" bindtap="bindTap" data-index="0">详 情</view>
      <view class="goods-tab-nav {{curIndex === 1 ?'on':''}}" bindtap="bindTap" data-index="1">参 数</view>
      <view class="goods-tab-nav {{curIndex === 2 ?'on':''}}" bindtap="bindTap" data-index="2">售 后</view>
      <view class="goods-tab-nav {{curIndex === 3 ?'on':''}}" bindtap="bindTaphaoping" data-index="3"
        data-maijia="{{goods['maijia']['title']}}" data-hpzt="" data-ksfyt="0">好 评</view>
    </view>
  </block>
  <block wx:else>
    <view wx:if="{{curIndex === 4}}" class="goods-tab-box">
      <button class="button-red" bindtap="bindTap" data-index="0">返 回</button>

    </view>
  </block>
