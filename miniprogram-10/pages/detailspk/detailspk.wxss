.to-carts-icon {
  position: absolute;
  right: 70rpx;
  top: 70rpx;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  opacity: .6;
  -webkit-animation: to_cart .3s ease-out;
  animation: to_cart .3s ease-out;

}

@-webkit-keyframes to_cart {
  0% {
    right: 100rpx;
    top: 530rpx;
    -webkit-transform: scale(4);
  }

  /*60%{
        top: 20rpx;
    }*/
}

@keyframes to_cart {
  0% {
    right: 100rpx;
    top: 530rpx;
